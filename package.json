{
  "name": "norm",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "prepare": "npm run build:npm 1.0.0 && npm run clean",
    "clean": "cpy . '../' --cwd=npm && rimraf npm/ src/",
    "format": "deno fmt",
    "build:npm": "deno run --allow-env --allow-net --allow-write --allow-read --allow-run  src/build_npm.ts",
    "lint": "deno lint",
    "test": "deno test -A"
  },
  "files": [
    "npm/",
    "src/"
  ],
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cpy-cli": "^4.2.0",
    "patch-package": "^6.5.1",
    "pg-structure": "^7.13.1",
    "rimraf": "^4.4.1",
    "zod": "^3.20.6"
  }
}
